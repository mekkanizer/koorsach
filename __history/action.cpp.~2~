#include <vcl.h>
#pragma hdrstop

#include "action.h"
#include "main.h"
//---------------------------------------------------------------------------
#pragma package(smart_init)
#pragma resource "*.dfm"
TForm4 *Form4;
//---------------------------------------------------------------------------
__fastcall TForm4::TForm4(TComponent* Owner)
	: TForm(Owner)
{
}
//---------------------------------------------------------------------------
void __fastcall TForm4::FormHide(TObject *Sender)
{
	Form1->Show();
}
//---------------------------------------------------------------------------

void __fastcall TForm4::FormShow(TObject *Sender)
{
	if (Form1->action == 1) {
		switch (Form1->object) {
			case 1:
			break;
			case 2:
			break;
			case 3:
			break;
			case 4:
			break;
		}
	}
}
//---------------------------------------------------------------------------
